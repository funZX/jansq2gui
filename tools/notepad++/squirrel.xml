<?xml version="1.0" encoding="Windows-1252" ?>
<NotepadPlus>
   <AutoComplete language="squirrel">
     <Environment ignoreCase="no" startFunc="(" stopFunc=")" paramSeparator="," terminal=";" additionalWordChar="."/>
          
	 <KeyWord name="jansq2gui__Api_Init" func="yes">
         <Overload retVal="" descr="Initialize 'jansq2gui` window and context.">
              <Param name="title" />
              <Param name="func" />
			  <Param name="width = 640" />
			  <Param name="height = 480" />
         </Overload>
     </KeyWord>
	 <KeyWord name="jansq2gui__Run" func="yes">
         <Overload retVal="" descr="Initialize main loop function.">
              <Param name="func" />
         </Overload>
     </KeyWord>
	 <KeyWord name="imgui.status" func="yes">
         <Overload retVal="{bool show, bool open}" descr="Initialize a status object for ImGui calls.">
         </Overload>
     </KeyWord>
	 <KeyWord name="imgui.vec2" func="yes">
         <Overload retVal="imgui.vec2(x, y)" descr="Instantiate a vec2 object.">
			<Param name="int x = 0" />
			<Param name="int y = 0" />
         </Overload>
     </KeyWord>
	 <KeyWord name="imgui.vec4" func="yes">
         <Overload retVal="(x, y, z, w)" descr="Instantiate a vec4 object.">
			<Param name="int x = 0" />
			<Param name="int y = 0" />
			<Param name="int z = 0" />
			<Param name="int w = 0" />
         </Overload>
     </KeyWord>
	 <KeyWord name="imgui.get_version" func="yes">
         <Overload retVal="const char*" descr="ImGui version.">
         </Overload>
     </KeyWord>	 
	 <KeyWord name="imgui.show_demo_window" func="yes">
         <Overload retVal="imgui.status" descr="Show ImGui demo window.">
			<Param name="bool open = false" />
         </Overload>
     </KeyWord>
	 <KeyWord name="imgui.begin" func="yes">
         <Overload retVal="imgui.status" descr="ImGui::Begin(...)">
			<Param name="const char* name" />
			<Param name="imgui.WindowFlags = 0" />
			<Param name="bool open = false" />
         </Overload>
     </KeyWord>
	 <KeyWord name="imgui.end" func="yes">
         <Overload retVal="" descr="ImGui::End()">
         </Overload>
     </KeyWord>
	 <KeyWord name="imgui.begin_child" func="yes">
         <Overload retVal="imgui.status" descr="ImGui::Begin(...)">
			<Param name="const char* str_id" />
			<Param name="imgui.vec2 size = (0, 0)" />
			<Param name="bool border = false" />
			<Param name="imgui.WindowFlags = 0" />
         </Overload>
     </KeyWord>
	 <KeyWord name="imgui.begin_child_id" func="yes">
         <Overload retVal="imgui.status" descr="ImGui::Begin(...)">
			<Param name="int id" />
			<Param name="imgui.vec2 size = (0, 0)" />
			<Param name="bool border = false" />
			<Param name="imgui.WindowFlags = 0" />
         </Overload>
     </KeyWord>
	 <KeyWord name="imgui.end_child" func="yes">
         <Overload retVal="" descr="ImGui::EndChild()">
         </Overload>
     </KeyWord>	 
	</AutoComplete>
</NotepadPlus>